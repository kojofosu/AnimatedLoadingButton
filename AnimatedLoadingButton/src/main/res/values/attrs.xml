<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ALBButton">
        <!--default-->
        <attr name="default_text" format="string"/>
        <attr name="default_bg_color" format="color"/>
        <attr name="default_text_color" format="color"/>

        <!--loading-->
        <attr name="loading_text" format="string"/>
        <attr name="loading_bg_color" format="color" />
        <attr name="loading_text_color" format="color" />

        <!--success-->
        <attr name="success_text" format="string" />
        <attr name="success_text_color" format="color"/>
        <attr name="success_bg_color" format="color"/>

        <!--error-->
        <attr name="error_text" format="string"/>
        <attr name="error_bg_color" format="color"/>
        <attr name="error_text_color" format="color" />







    </declare-styleable>
</resources>